{
  "name": "backend",
  "main": "./dist/index",
  "license": "ISC",
  "scripts": {
    "start": "export NODE_ENV=production; node ./dist/index",
    "dev": "export NODE_ENV=development; node ./dist/index",
    "watch": "nodemon -L -w src -e ts,js -x \"npm run serve\"",
    "serve": "tsc && export NODE_ENV=development; node --inspect=0.0.0.0:9229 dist/index",
    "build": "npm run lint && tsc",
    "lint": "eslint --fix --ext ts .",
    "clean": "rm -Rf ./dist",
    "rebuild": "npm run clean && npm run build",
    "pretest": "export NODE_ENV=test; sequelize db:drop; sequelize db:create; sequelize db:migrate;",
    "test": "export NODE_ENV=test; jest --coverage --verbose"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.1",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.5",
    "sequelize-typescript": "^1.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/helmet": "0.0.45",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.9.1",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nan": "^2.14.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
